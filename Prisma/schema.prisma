// generator
generator client {
    provider = "prisma-client-py"
    recursive_type_depth = 5
}

// database
datasource db {
    provider = "postgresql"
    url = env("DATABASE_URL")
}

// data models
model data_crypto_24h{
    id Int @id @default(autoincrement())
    symbol String @map("symbol") @db.VarChar(20)
    weightedavgprice Float @map("weightedavgprice") @db.Real
    prevcloseprice Float @map("prevcloseprice") @db.Real
    highprice Float @map("highprice") @db.Real
    lowprice Float @map("lowprice") @db.Real
    date_info String @map("date_info") @db.VarChar(255)

    @@map("data_crypto_24h") // name table in postgresql
}
